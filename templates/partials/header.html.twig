<header class="text-gray-600 body-font">
    <div class="container mx-auto flex flex-wrap p-5 flex-col md:flex-row items-center">
        <a class="flex title-font font-medium items-center text-gray-900 mb-4 md:mb-0">
            <img class="h-16 w-52 rounded-xl" src="data:image/webp;base64," alt="">
        </a>
        <nav class="md:ml-auto md:mr-auto flex flex-wrap items-center text-base justify-center">
            <a href="{{ path('app_home') }}" class="headerNav mr-5 cursor-pointer ">Accueil</a>
            <a class="headerNav mr-5 cursor-pointer">Actualités</a>
            <a href="{{ path('app_library_lol') }}" class="headerNav mr-5 cursor-pointer">L'histoire</a>
            <a class="headerNav mr-5 cursor-pointer">Swipe..</a>
        </nav>
        {%- if is_granted('ROLE_USER') -%}
        <a href="{{ path('app_logout') }}" class="firstButton inline-flex items-center border-0 py-2 px-5 focus:outline-none rounded text-base text-white mt-4 hover:bg-opacity-80">
            Déconnexion
        </a>
        {%- else -%}
        <a href="{{ path('app_login') }}" class="firstButton inline-flex items-center border-0 py-2 px-5 focus:outline-none rounded text-base text-white mt-4 hover:bg-opacity-80">
            Se connecter
        </a>
        {%- endif -%}
    </div>
</header>